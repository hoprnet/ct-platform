---
apiVersion: rabbitmq.com/v1beta1
kind: Vhost
metadata:
  name: ctapp-staging
spec:
  name: ctapp-staging
  rabbitmqClusterReference:
    name: rabbitmq-ha-cluster
---
apiVersion: rabbitmq.com/v1beta1
kind: User
metadata:
  name: ctapp-staging
spec:
  importCredentialsSecret:
    name: ctapp-rabbitmq-staging
  rabbitmqClusterReference:
    name: rabbitmq-ha-cluster
---
apiVersion: rabbitmq.com/v1beta1
kind: Permission
metadata:
  name: ctapp-staging
spec:
  permissions:
    configure: .*
    read: .*
    write: .*
  userReference:
    name: ctapp-staging
  vhost: ctapp-staging
  rabbitmqClusterReference:
    name: rabbitmq-ha-cluster